# Uncomment the next line to define a global platform for your project

target 'YAD' do
  platform :ios, '11.0'
  use_frameworks!

  pod 'CryptoSwift' 
  
  target 'YADTests' do 
    inherit! :search_paths
    pod 'CryptoSwift'
  end

end

target 'YADmac' do
  platform :macos, '10.12'
  use_frameworks!

  pod 'CryptoSwift' 
  
  target 'YADmacTests' do 
    inherit! :search_paths
    pod 'CryptoSwift'
  end

end
